    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "typeorm": "ts-node -r tsconfig-paths/register --project tsconfig.typeorm.json ./node_modules/typeorm/cli.js",
    "migration:deploy": "yarn prisma migrate deploy",
    "migration:dev": "yarn prisma migrate dev",
    "migration:generate": "yarn prisma migrate dev --create-only",
    "migration:reset": "yarn prisma migrate reset",
    "migration:status": "yarn prisma migrate status",
    "seed:run": "yarn prisma db seed",
    "seed:fresh": "rm ./prisma/db-test.sqlite && yarn run migration:dev && yarn run seed:run",
    "analyze:client": "webpack-bundle-analyzer dist/apps/client/stats.json",
    "release": "release-it",
    "prepare": "husky install"
  },
